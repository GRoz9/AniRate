                        <div class="TagBox"><%= Respone.season.media[i].genres[j] %></div>
                    <% } %>
                </div>
            </div>
            </a>
            <a href="/<%= Respone.season.media[i].type%>/<%= Respone.season.media[i].id %>/<%= encodeURI(Respone.season.media[i].title[Lang]) %>" class="title"><%= Respone.season.media[i].title[Lang] %></a>
        </div>
    <% } %>
