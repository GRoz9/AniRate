<div class="container">
        
    <!-- Trending -->
    <div class="categoryTitle" id="Trending">
    <a href="<%= Respone.trending.media[0].type%>/Trending" class="Hover">Trending</a>
    <a href="<%= Respone.trending.media[0].type%>/Trending" class="Hover">View More</a>
    </div>

    <section class="Catagories">
    <% for (let i = 0; i < 6; i++) { %>
        <div class="box">
            <a href="/<%= Respone.trending.media[i].type%>/<%= Respone.trending.media[i].id %>/<%= encodeURI(Respone.trending.media[i].title[Lang]) %>" class="cover"> 
            <img src=<%= Respone.trending.media[i].coverImage.extraLarge %> alt="" class="image loaded" />
            <div class="overview">
                <div class="overviewAvg">Average Score: <%= Respone.trending.media[i].averageScore %>%</div>
                <div class="overviewDescription"><%= cleanDesc(Respone.trending.media[i].description, 118) %> </div>
                <div class="ExtraInfo">
                    <p>Start Date: <%= cleanDate(Respone.trending.media[i].startDate.month, Respone.trending.media[i].startDate.day, Respone.trending.media[i].startDate.year) %></p>
                    <p>Status: <%= capitalise(Respone.trending.media[i].status) %> </p>
                    <p>Season: <%= capitalise(Respone.trending.media[i].season + " " + Respone.trending.media[i].seasonYear) %></p>
                </div>
                <div class="Info">
                    <span><%= Respone.trending.media[i].format %> Show</span>
                    <span class="Seperator">•</span>
                    <span><%= Respone.trending.media[i].episodes %> Episodes</span>
                </div>
                <div class="Tags">
                    <!-- for (let j = 0; j < Math.min(((Respone.trending.media[i].genres).length), 3); j++) -->
                    <% for (let j = 0; j < Math.min(((Respone.trending.media[i].genres).length), 3); j++) { %>
                        <div class="TagBox"><%= Respone.trending.media[i].genres[j] %></div>
                    <% } %>
                </div>
            </div>
            </a>
            <a href="/<%= Respone.trending.media[i].type%>/<%= Respone.trending.media[i].id %>/<%= encodeURI(Respone.trending.media[i].title[Lang]) %>" class="title"><%= Respone.trending.media[i].title[Lang] %></a>
        </div>
    <% } %>
    </section>
    <!-- Trending -->

    <!-- Popular This Season-->
    <div class="categoryTitle" id="PopularThisSeason">
    <a href="<%= Respone.season.media[1].type%>/This-Season" class="Hover">Popular This Season</a>
